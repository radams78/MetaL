@Article{DBLP:journals/amai/Arzi-GonczarowskiL98,
  Title                    = {Introducing the Mathematical Category of Artificial Perceptions},
  Author                   = {Zippora Arzi-Gonczarowski and Daniel J. Lehmann},
  Journal                  = {Ann. Math. Artif. Intell.},
