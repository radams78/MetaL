@Misc{Ishizaka2009,
  Title                    = {Quantum Teleportation Scheme by Selecting One of Multiple Output Ports},

  Author                   = {Satoshi Ishizaka and Tohya Hiroshima},
  Year                     = {2009},

  Abstract                 = {The scheme of quantum teleportation, where Bob has multiple (N) output ports and obtains the teleported state by simply selecting one of the N ports, is thoroughly studied. We consider both the deterministic version and the probabilistic version of the teleportation scheme aiming to teleport an unknown state of a qubit. Moreover, we consider two cases for each version: (i) the state employed for the teleportation is fixed to a maximally entangled state, and (ii) the state is also optimized as well as Alice's measurement. We analytically determine the optimal protocols for all the four cases, and show the corresponding optimal fidelity or optimal success probability. All these protocols can achieve the perfect teleportation in the asymptotic limit of N -> infinity. The entanglement properties of the teleportation scheme are also discussed.},
  Keywords                 = {port-based teleportation},
  Owner                    = {robin},
  Timestamp                = {2014.10.17}
}

